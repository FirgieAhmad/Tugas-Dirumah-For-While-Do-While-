#include <iostream>
using namespace std;
// Nama : Firgie Ahmad ==== NIM : ETE10190123
// Tugas Rumah 31 Maret 2020 === (For, While, D0 While)
int main () {
	// Menampilkan angka ganjil genap 11-1100
	int angka_awal;
	cout << "Masukkan angka dari range 11-1100 : " << endl;
	cin >> angka_awal;
	
	for ( int i = 11; i <= 1100; i++ ){
		if ( i % 2 == 1 ){
			cout << i << " : Adalah bilangan ganjil " <<endl ;		
		}else{
			cout << i << " : Adalah bilangan genap " <<endl ;
		}
	
	}

}
